# Male offenders in Australia
male_aus <- read_excel("Data/1. Offenders, Australia.xls", sheet = 3, range = "A5:M22")

male_aus <- male_aus %>% rename("offence_male" = "...1",
                                "2008" = "Number",
                                "2009" = "...3",
