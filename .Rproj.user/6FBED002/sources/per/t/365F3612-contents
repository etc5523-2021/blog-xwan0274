data_zb <- read_excel("Data/Gender_StatsEXCEL.xlsx") %>% 
  rename("Indicator" = "Indicator Name",
         "Indicator_code" = "Indicator Code", 
         "Country_name" = "Country Name") %>% 
  filter(Indicator_code %in% selected_indicators) %>% 
  select(!("1960":"1999"))

data_zb_tidy <- data_zb %>% 
  select(-"Indicator") %>% 
  pivot_longer(!("Country_name":"Indicator_code"), 
                 names_to = "Year",
                 values_to = "Values") %>% 
  pivot_wider(names_from = "Indicator_code",
              values_from = "Values") 

data_income <- read_excel("Data/CLASS.xls", range = anchored("C5", dim = c(220,5)))

new_name = c("can_open_account",
                        "can_reg_business",
                        "can_sign_contract",
                        "account_at_FI_female",
                        "account_at_FI_male",
                        "borrowed_female",
                        "borrowed_male",
                        "borrowed_to_start_female",
                        "borrowed_to_start_male",
